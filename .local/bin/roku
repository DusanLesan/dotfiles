#!/bin/sh

case $1 in
	"profiler") ~/.local/share/applications/appimages/bsprof-visualization-tool-linux64.AppImage ;;
	"stream") ~/.local/share/applications/appimages/roku-stream-tester-linux64.AppImage ;;
	"deeplink") ~/.local/share/applications/appimages/roku-deep-linking-tester-linux64.AppImage ;;
	*) ~/.local/share/applications/appimages/roku-advanced-layout-editor-linux64.AppImage ;;
esac
