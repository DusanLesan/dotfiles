preview any_text text/* {{
	bat -p --color=always "${f}" --theme 'Visual Studio Dark+' --tabs 2
}}

preview zip */zip {{
	unzip -l "${f}"
}}

preview audio audio/* {{
	if exists ffmpeg; then
		meta="$(ffmpeg -hide_banner -i "$f" 2>&1)"
		printf '%s\n' "$meta"
		y=$((y + $(printf '%s\n' "$meta" | wc -l)))
	fi
}}
